
package latihan4a;

public class Latihan4A {
    
    public static void main(String[] args) {
        
        int sukuawal = 5;
        int beda = 5;
        int hasil;
        
        for (int a = 0; a <= 4; a++){
            for (int i = 1; i <= a; i++){
                
                hasil = sukuawal;
                sukuawal += beda;
                
                System.out.print(" "+hasil);
            }
            System.out.println(" ");
        }
        
    }
    
}
